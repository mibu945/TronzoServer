{
  "name": "lookup-server",
  "version": "0.0.1",
  "private": true,
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "body-parser": "^1.16.1",
    "express": "4.13.3",
    "jwt-simple": "^0.5.1",
    "mongoose": "^4.7.6",
    "nodemon": "^1.11.0"
  },
  "scripts": {
    "start": "concurrently \"npm run mongoDB\" \"npm run server\"",
    "mongoDB": "mongod --dbpath=mongoDB/data",
    "server": "nodemon server.js --exec babel-node"
  },
  "devDependencies": {
    "concurrently": "^3.1.0"
  }
}
